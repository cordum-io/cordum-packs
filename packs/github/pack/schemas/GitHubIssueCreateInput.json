{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GitHub Issue Create Input",
  "type": "object",
  "properties": {
    "profile": {"type": "string"},
    "owner": {"type": "string"},
    "repo": {"type": "string"},
    "repository": {"type": "string"},
